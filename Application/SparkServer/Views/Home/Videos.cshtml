@model SparkServer.ViewModels.HomeVideoViewModel
@{
    ViewBag.Title = "Videos";
}

<h1>@ViewBag.Title</h1>

<ul class="blog-list video-list">

    @for (var i = 0; i < Model.VideoList.Count; i++)
    {
        <li class="cf">
            <a href="@Model.VideoList[i].VideoURL"><img src="@Model.VideoList[i].ThumbnailURL" alt="@Model.VideoList[i].Title Thumbnail" /></a>
            <h3><a href="@Model.VideoList[i].VideoURL">@Model.VideoList[i].Title</a></h3>
            <p>@Model.VideoList[i].Subtitle<p>
        </li>
    }

</ul>